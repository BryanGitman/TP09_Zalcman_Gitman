@{
    ViewData["Title"] = "Iniciar Sesion";
}
<body>
    <div class="logoIS"><img src="/LogoProgViaje.png"></div>
    <form method="post" action='@Url.Action("SesionIniciada","Home")'>
      <input name="NombreUsuario" class="name" type="text" placeholder="Nombre de Usuario">
      <br>
      <input name="Contraseña" class="contra" type="password" placeholder="Contraseña">
      <p class="text-muted text-center">¿No tenés una cuenta? <a class="atajologin" href="Registrarse">Registrate</a></p>  
      <input type="submit" class="btn btn-success submitIS" value="Iniciar Sesion">     
     </form>
</body>